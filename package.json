    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "eslint": "9.22.0",
    "eslint-config-next": "^15.2.4",
    "eslint-plugin-react": "^7.37.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
