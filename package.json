    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.3-rc.5",
    "@radix-ui/themes": "^3.2.1",
    "@tanstack/react-virtual": "^3.13.4",
    "bright": "^1.0.0",
    "date-fns": "^4.1.0",
    "isomorphic-dompurify": "^2.22.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.483.0",
    "md5": "^2.3.0",
    "mystjs": "^0.0.15",
    "next": "^15.2.2",
    "next-themes": "^0.4.4",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0"
