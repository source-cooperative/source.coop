{
  "name": "source-cooperative",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "./scripts/dev.sh",
    "dev:next": "next dev",
    "dev:tunnel": "ory tunnel --dev --allowed-cors-origins=\"http://localhost:3000\" --debug --project ff8f07bb-adb7-4399-af07-20e633a105f6 http://localhost:3000",
    "build": "next build",
    "start": "next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=false next lint",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false next lint --fix",
    "lint:file": "ESLINT_USE_FLAT_CONFIG=false eslint",
    "lint:src": "ESLINT_USE_FLAT_CONFIG=false eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:src:fix": "ESLINT_USE_FLAT_CONFIG=false eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "init-local": "ts-node scripts/init-local.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --runInBand",
    "test-pages": "node scripts/test-pages.js",
    "test:perf": "node scripts/test-perf.js",
    "setup-test-data": "ts-node scripts/setup-test-data.ts",
    "start-dynamodb": "./scripts/start-dynamodb.sh",
    "check-dynamodb": "node scripts/check-dynamodb.js",
    "predev": "npm run check-dynamodb || (echo 'DynamoDB not running. Starting DynamoDB...' && npm run start-dynamodb -- --no-init)"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.767.0",
